<article class="artworklist">
	<h1>Image Gallery</h1>
	<p>Check out our image gallery</p>
	<div class="pixgrid">
		<% if (artwork.length > 0) { %>
			<% for (i=0; i<artwork.length; i++) { %>
				<img src="/images/artwork/<%= artwork[i]%>" alt="Artwork <%= i%>">
			<% } %>
		<% } %>
	</div>
</article><!-- article -->